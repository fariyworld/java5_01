<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.offcn.mappers.IdleTypeMapper">
		
	<select id="selectAll" resultType="IdleType">
		SELECT
		  it.tid, 
		  it.typename 
		FROM
		  idletype it 
	</select>
	
</mapper>